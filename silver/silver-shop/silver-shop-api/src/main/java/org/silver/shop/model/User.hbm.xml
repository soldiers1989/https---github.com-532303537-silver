<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

      <hibernate-mapping package="org.silver.shop.model">
          
          <class name="User" table="ym_shop_user" >
          
          <id name="id" type="long"> 
              <column name="id" />              
               <generator class="identity">           
                
            </generator>       
          </id>
         
          <property name="user_id">
              <column name="user_id" not-null="true" unique="true"/>
         </property>
         
          <property name="login_pass">
              <column name="login_pass" not-null="true"/>
         </property>
         
         <property name="address">
              <column name="address" />
         </property>
        
         <property name="mobile">
              <column name="mobile" />
         </property>
        
           <property name="personal_ID">
              <column name="personal_ID" />
         </property>
        
          <property name="del_flag" type="java.lang.Integer">
              <column name="del_flag" not-null="true"/>
         </property>
        
           <property name="create_date" type="java.util.Date">
              <column name="create_date" not-null="true"/>
            </property>
        
           <property name="create_by">
              <column name="create_by" />
            </property>
        
        <property name="update_date" type="java.util.Date">
              <column name="update_date"/>
            </property>
        
         <property name="update_by">
              <column name="update_by" />
            </property>
        
        <property name="remarks">
              <column name="remarks" />
            </property>
        
        
        
          </class>
          
        </hibernate-mapping>